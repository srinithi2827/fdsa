import numpy as np
import matplotlib.pyplot as plt

# Compute x values
x = np.arange(0, 3 * np.pi, 0.1)

# Compute sine and cosine values
y_sin = np.sin(x)
y_cos = np.cos(x)

# Plot the curves
plt.plot(x, y_sin)
plt.plot(x, y_cos)

plt.xlabel('x axis label')
plt.ylabel('y axis label')
plt.title('Sine and Cosine')

plt.legend(['Sine', 'Cosine'])

plt.show()
