from matplotlib import pyplot as plt

x = [5, 2, 9, 4, 7]
y = [10, 5, 8, 4, 2]

# Line Plot
plt.subplot(2,2,1)
plt.plot(x, y)
plt.title("Line Plot")

# Bar Plot
plt.subplot(2,2,2)
plt.bar(x, y)
plt.title("Bar Plot")

# Histogram
plt.subplot(2,2,3)
plt.hist(y)
plt.title("Histogram")

# Scatter Plot
plt.subplot(2,2,4)
plt.scatter(x, y)
plt.title("Scatter Plot")

plt.tight_layout()
plt.show()
